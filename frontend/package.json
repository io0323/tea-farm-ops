    "axios": "^1.6.2",
    "nth-check": "^2.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.20.1",
    "react-scripts": "^5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@svgr/plugin-svgo": "^8.1.0",
    "@svgr/webpack": "^8.1.0",
    "@testing-library/cypress": "^10.0.1",
    "@types/testing-library__jest-dom": "^5.14.9",
    "cypress": "^13.6.1",
    "jest": "^27.5.1",
    "jest-environment-jsdom": "^27.5.1",
    "prettier": "^3.6.2",
    "resolve-url-loader": "^5.0.0",
